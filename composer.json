{
    "name": "mtolhuijs/laravel-database-synchronizer",
    "description": "Synchronize your production and development databases with a simple command",
    "license": "MIT",
    "authors": [
        {
            "name": "Maarten Tolhuijs",
            "email": "mtolhuys@hotmail.com",
            "homepage": "https://github.com/mtolhuys/laravel-database-synchronize"
        }
    ],
    "homepage": "https://github.com/mtolhuys/laravel-database-synchronizer",
    "keywords": ["Laravel", "laravel-database-synchronizer"],
    "require": {
        "illuminate/support": "~5|~6|~7",
        "doctrine/dbal":"^2.3",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0|~8.0|~9.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "mtolhuijs\\LDS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mtolhuijs\\LDS\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "mtolhuijs\\LDS\\DatabaseSynchronizerServiceProvider"
            ]
        }
    }
}
